{
    "name": "BaseProject",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "clean:android": "cp envs/.env.dev .env && rm -rf android/app/build && cd android && ./gradlew clean && cd ..",
        "clean:ios": "cd ios && pod cache clean --all && rm -rf build && rm -rf Pods && rm -rf Podfile.lock && RCT_NEW_ARCH_ENABLED=0 bundle exec pod install && cd ..",
        "run:android_dev": "cp envs/.env.dev .env & npx react-native run-android --mode devDebug",
        "run:android_stag": "cp envs/.env.stag .env && npx react-native run-android --mode 'stagDebug'",
        "run:android_prod": "cp envs/.env.prod .env && npx react-native run-android --mode 'prodDebug'",
        "build:android_dev": "cp envs/.env.dev .env && cd android && ./gradlew assembleDevRelease && cd ..",
        "build:android_stag": "cp envs/.env.stag .env && cd android && ./gradlew assembleStagRelease && cd ..",
        "build:android_prod:aab": "cp envs/.env.prod .env && rm -rf android/app/build && cd android && ./gradlew bundleProdRelease && cd ..",
        "run:ios_dev": "npx react-native run-ios --scheme 'AppDev'",
        "run:ios_stag": "npx react-native run-ios --scheme 'AppStag'",
        "run:ios_prod": "npx react-native run-ios --scheme 'AppProd'",
        "start": "react-native start --reset-cache",
        "test": "jest",
        "lint": "eslint . --fix",
        "postinstall": "patch-package",
        "ios:pod:install": "cd ios && RCT_NEW_ARCH_ENABLED=0 bundle exec pod install"
    },
    "dependencies": {
        "@bam.tech/react-native-image-resizer": "^3.0.11",
        "@react-native-async-storage/async-storage": "^2.1.2",
        "@react-native-community/hooks": "^3.0.0",
        "@react-navigation/bottom-tabs": "^7.2.0",
        "@react-navigation/native": "^7.0.14",
        "@react-navigation/native-stack": "^7.2.0",
        "@react-navigation/stack": "^7.1.1",
        "@reduxjs/toolkit": "^2.5.0",
        "axios": "^1.7.9",
        "dayjs": "^1.11.13",
        "i18next": "^24.2.0",
        "lodash": "^4.17.21",
        "react": "18.3.1",
        "react-i18next": "^15.2.0",
        "react-native": "0.76.5",
        "react-native-code-push": "^9.0.1",
        "react-native-config": "^1.5.3",
        "react-native-date-picker": "^5.0.8",
        "react-native-device-info": "^14.0.2",
        "react-native-fast-image": "^8.6.3",
        "react-native-fast-shadow": "^0.1.1",
        "react-native-gesture-handler": "^2.21.2",
        "react-native-image-crop-picker": "^0.41.6",
        "react-native-keyboard-aware-scroll-view": "^0.9.5",
        "react-native-modal": "^13.0.1",
        "react-native-reanimated": "^3.16.6",
        "react-native-safe-area-context": "^5.0.0",
        "react-native-screens": "^4.4.0",
        "react-native-svg": "^15.10.1",
        "react-native-tab-view": "^4.0.5",
        "react-native-toast-notifications": "^3.4.0",
        "react-redux": "^9.2.0",
        "react-use": "^17.6.0",
        "redux-persist": "^6.0.0",
        "use-dimensions": "^2.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@babel/runtime": "^7.25.0",
        "@eslint/compat": "^1.2.4",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.17.0",
        "@react-native-community/cli": "15.0.1",
        "@react-native-community/cli-platform-android": "15.0.1",
        "@react-native-community/cli-platform-ios": "15.0.1",
        "@react-native-community/eslint-config": "^3.2.0",
        "@react-native/babel-preset": "0.76.5",
        "@react-native/eslint-config": "0.76.5",
        "@react-native/metro-config": "0.76.5",
        "@react-native/typescript-config": "0.76.5",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.13",
        "@types/qs": "^6.9.17",
        "@types/react": "^18.2.6",
        "@types/react-native": "^0.73.0",
        "@types/react-test-renderer": "^19.0.0",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "babel-jest": "^29.6.3",
        "babel-plugin-module-resolver": "^5.0.2",
        "deprecated-react-native-prop-types": "^5.0.0",
        "eslint": "^9.17.0",
